<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- Browser values! Local: firefox, iexplore, chrome, htmlunit Remote: remotechrome, remotefx, remoteie, remoteunitjs, remotephantom, remoteunit-->
    <add key="client_browser" value="chrome"/>
    <!-- Standard waiting time (implicit wait) -->
    <add key="waiting_time" value="25"/>
    <add key="ScreenshotPath" value="/"/>
  </appSettings>
  <connectionStrings>
    <add name="SeleniumServerUrl" connectionString="http://urltoseleniumserver:8080"/>
    <add name="PhantomServerUrl" connectionString="http://urltophantomserver:8081"/>
    <add name="SeleniumTestUrl" connectionString="http://www.google.se"/>
  </connectionStrings>
</configuration>
